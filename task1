import sys

def circular_array_path(n, m):
    # Создаем круговой массив от 1 до n
    circular_array = list(range(1, n + 1))
    
    # Инициализируем путь
    path = []
    
    # Начальная позиция
    current_position = 0
    
    while True:
        # Добавляем начальный элемент текущего интервала в путь
        path.append(circular_array[current_position])
        
        # Вычисляем следующую позицию
        current_position = (current_position + m - 1) % n
        
        # Проверяем, вернулись ли мы к первому элементу
        if current_position == 0:
            break
    
    # Преобразуем путь в строку и выводим результат
    print(''.join(map(str, path)))

if __name__ == "__main__":
    # Получаем аргументы командной строки
    if len(sys.argv) != 3:
        print("Usage: python script.py <n> <m>")
        sys.exit(1)
    
    n = int(sys.argv[1])
    m = int(sys.argv[2])
    
    circular_array_path(n, m)
